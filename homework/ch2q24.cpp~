#include <iostream>
#include <cmath>
using namespace std;

class INT
{
public:
  INT( int x )
    : data(x){ }
  INT( ) { }
  
  //binary operators
  INT operator+(INT lhs, INT rhs) const
  {
    return lhs.data + rhs.data;
  }
  INT operator*(INT lhs, INT rhs) const
  {
    return lhs.data * rhs.data;
  }
  INT operator ^(INT lhs, INT rhs) const
  {
    return pow(lhs.data,rhs.data);
  }



private:
  int data;

};
